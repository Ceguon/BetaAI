{
  "manifest_version": 2,
  "name": "OpenDefineAct",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],

  "permissions": ["contextMenus", "activeTab", "storage"]
  }

